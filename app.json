{
  "expo": {
    "name": "Todo List",
    "slug": "todo-list",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Full-stack todo list React Native app with Facebook Auth and Firebase",
    "githubUrl": "https://github.com/nycodinguru/react-native-fullstack-todo-list"
  }
}